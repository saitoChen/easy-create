{
  "name": "create-easy",
	"displayName": "Create-Easy",
	"description": "easy to create a file/folder",
	"publisher": "chenjianfeng",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
    "*"
	],
	"icon": "img/easycreate.png",
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "create-easy.easyCreate",
			"title": "Create File/Folder"

		}],
		"menus": {
			"explorer/context": [
				{
					"command": "create-easy.easyCreate",
					"group": "navigation"
				}
			]
		},
		"keybindings": [{
			"command": "create-easy.easyCreate",
			"key": "ctrl+shift+c",
			"mac": "cmd+shift+c"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.51.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }

}
